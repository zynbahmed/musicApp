<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Release Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% albums.forEach(function(a) { %>
      <tr>
        <td><%= a.title %></td>
        <td><%= a.artist %></td>
        <td><%= a.releaseYear %></td>
        <td><a href="/albums/<%= a._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
